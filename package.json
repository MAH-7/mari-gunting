{
  "name": "mari-gunting-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "customer": "cd apps/customer && npm start",
    "provider": "cd apps/provider && npm start",
    "install:all": "npm install && npm run install:customer && npm run install:provider && npm run install:shared",
    "install:customer": "cd apps/customer && npm install",
    "install:provider": "cd apps/provider && npm install",
    "install:shared": "cd packages/shared && npm install"
  },
  "devDependencies": {
    "typescript": "~5.9.2"
  }
}
