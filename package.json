{
  "name": "mari-gunting-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "customer": "cd apps/customer && npm start",
    "partner": "cd apps/partner && npm start",
    "install:all": "npm install --legacy-peer-deps && npm run install:customer && npm run install:partner && npm run install:shared",
    "install:customer": "cd apps/customer && npm install --legacy-peer-deps",
    "install:partner": "cd apps/partner && npm install --legacy-peer-deps",
    "install:shared": "cd packages/shared && npm install --legacy-peer-deps",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "devDependencies": {
    "typescript": "~5.9.2"
  },
  "dependencies": {
    "@react-native-picker/picker": "^2.11.2",
    "dotenv": "^17.2.3",
    "expo": "~54.0.12",
    "expo-audio": "~1.0.15",
    "expo-image-manipulator": "~14.0.7",
    "expo-notifications": "^0.32.12",
    "expo-task-manager": "~14.0.8",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-razorpay": "^2.3.0",
    "react-native-webview": "^13.16.0",
    "react-native-worklets": "^0.6.1",
    "react-native-worklets-core": "^1.6.2"
  }
}
