{
  "name": "mari-gunting-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "customer": "cd apps/customer && npm start",
    "partner": "cd apps/partner && npm start",
    "install:all": "npm install && npm run install:customer && npm run install:partner && npm run install:shared",
    "install:customer": "cd apps/customer && npm install",
    "install:partner": "cd apps/partner && npm install",
    "install:shared": "cd packages/shared && npm install"
  },
  "devDependencies": {
    "typescript": "~5.9.2"
  },
  "dependencies": {
    "@react-native-picker/picker": "^2.11.2",
    "dotenv": "^17.2.3"
  }
}
