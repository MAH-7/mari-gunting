{
  "expo": {
    "name": "Mari Gunting Partner",
    "slug": "mari-gunting-partner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marigunting.app",
      "deploymentTarget": "15.1",
      "backgroundModes": [
        "location",
        "fetch"
      ],
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Mari Gunting needs your location to show your availability to nearby customers and navigate to their locations.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Mari Gunting needs continuous location access to keep you visible to customers even when the app is minimized. This helps you receive booking requests and provides real-time location updates to customers.",
        "NSLocationAlwaysUsageDescription": "Mari Gunting needs continuous location access to keep you visible to customers even when the app is minimized. This helps you receive booking requests and provides real-time location updates to customers.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.marigunting.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Mari Gunting needs continuous location access to keep you visible to customers even when the app is minimized. This helps you receive booking requests and provides real-time location updates to customers.",
          "isAndroidBackgroundLocationEnabled": true,
          "isIosBackgroundLocationEnabled": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "scheme": "marigunting",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "appEnv": "development",
      "enableAnalytics": false,
      "enableErrorTracking": false,
      "enablePushNotifications": false,
      "enableChat": false,
      "enableVideoCall": false,
      "commissionRate": 0.15,
      "cancellationWindow": 24,
      "defaultSearchRadius": 10,
      "router": {},
      "eas": {
        "projectId": "dd2da2ed-7f01-4dc7-8af5-708ffa6d2930"
      }
    }
  }
}
